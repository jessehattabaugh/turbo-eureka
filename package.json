{
	"devDependencies": {
		"@eslint/compat": "latest",
		"@eslint/eslintrc": "latest",
		"@eslint/js": "latest",
		"express": "",
		"eslint": "latest",
		"globals": "latest",
		"matter-js": "latest",
		"parcel": "latest",
		"surge": "latest"
	},
	"scripts": {
		"build:prod": "parcel build ./www/*.html --dist-dir ./dist --public-url './'",
		"build:staging": "parcel build ./www/*.html --dist-dir ./dist --public-url './' --no-optimize",
		"deploy:prod": "npm run build:prod && surge ./dist --domain turbo-eureka.surge.sh",
		"deploy:staging": "npm run build:staging && surge ./dist --domain staging-turbo-eureka.surge.sh",
		"kill": "npx kill-port 3000",
		"serve:parcel": "parcel serve ./www/*.html --port 3000"
	},
	"type": "module"
}
